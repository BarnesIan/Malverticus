{
    
    "manifest_version": 2 ,
    "name": "Malverticus",
    "version": "1.0",
    "description": "Malverticus the Malvertisement and Malicious URL Blocker",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "tabs",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "icons": {
        "16": "icons/logo.png",
        "48": "icons/logo.png",
        "128": "icons/logo.png"
    }

}